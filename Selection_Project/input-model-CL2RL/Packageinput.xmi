<?xml version="1.0" encoding="UTF-8"?>
<Class:Package xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:Class="http://www.example.org/Class" xsi:schemaLocation="http://www.example.org/Class Class.ecore">
  <elems xsi:type="Class:DataType"
      name="String"/>
  <elems xsi:type="Class:DataType"
      name="Integer"/>
  <elems xsi:type="Class:Class"
      name="Family">
    <att name="name"
        type="//@elems.0"/>
    <att name="members"
        multiValued="true"
        type="//@elems.3"/>
  </elems>
  <elems xsi:type="Class:Class"
      name="Person">
    <att name="firstName"
        type="//@elems.0"/>
    <att name="closestFriend"
        type="//@elems.3"/>
    <att name="emailAddresses"
        multiValued="true"
        type="//@elems.0"/>
  </elems>
</Class:Package>
